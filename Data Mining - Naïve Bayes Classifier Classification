   "source": [
    "# Check multicollinearity using VIF\n",
    "b = df[['Emergency Admission', 'Population', 'Intravenous', 'Children', 'CT Scan', 'TotalCharge', 'Initial_days']]\n",
    "\n",
    "vif_data = pd.DataFrame()\n",
    "vif_data['Feature'] = b.columns\n",
    "vif_data['VIF'] = [variance_inflation_factor(b.values, i) for i in range(len(b.columns))]\n",
    "vif_data.sort_values('VIF', axis=0, ascending=False, inplace=True)\n",
    "vif_data"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "id": "43d80dba",
   "metadata": {
    "scrolled": true
   },
   "outputs": [
