   "cell_type": "markdown",
   "id": "ba1ee277",
   "metadata": {},
   "source": [
    "#### A4. SQL Code<a id='a4'></a>\n",
    "\n",
    "`-- Table: public.kaggle`\n",
    "\n",
    "`-- DROP TABLE public.kaggle;`\n",
    "\n",
    "CREATE TABLE public.kaggle\n",
    "(\n",
    "    kaggle_id integer NOT NULL GENERATED ALWAYS AS IDENTITY ( INCREMENT 1 START 1 MINVALUE 1 MAXVALUE 2147483647 CACHE 1 ),</br>\n",
    "    gender text COLLATE pg_catalog.\"default\" NOT NULL,</br>\n",
    "    age double precision NOT NULL,</br>\n",
    "    hypertension integer NOT NULL,</br>\n",
    "    heart_disease integer NOT NULL,</br>\n",
    "    smoking_history text COLLATE pg_catalog.\"default\" NOT NULL,</br>\n",
    "    bmi double precision NOT NULL,</br>\n",
    "    a1c double precision NOT NULL,</br>\n",
    "    blood_glucose_level integer NOT NULL,</br>\n",
    "    diabetes integer NOT NULL,</br>\n",
    "    CONSTRAINT kaggle_id PRIMARY KEY (kaggle_id)</br>\n",
    ")\n",
    "\n",
    "TABLESPACE pg_default;\n",
    "\n",
    "ALTER TABLE public.kaggle\n",
    "    OWNER to postgres;\n",
    "\t\n",
    "`-- IMPORTING KAGGLE CSV FILE TO KAGGLE TABLE`</br>\n",
    "COPY PUBLIC.KAGGLE (GENDER, AGE, HYPERTENSION, HEART_DISEASE, SMOKING_HISTORY, BMI, A1C, BLOOD_GLUCOSE_LEVEL, DIABETES)</br>\n",
    "FROM 'C:/Users/Public/DOWNLO~1/DIABET~1.CSV' </br>\n",
    "DELIMITER ',' </br>\n",
    "CSV HEADER ;\n",
    "\n",
    "`-- CHANGING AGE DATA TYPE TO INTEGER IN KAGGLE TABLE`</br>\n",
    "ALTER TABLE KAGGLE</br>\n",
    "ALTER COLUMN AGE TYPE INTEGER;</br>\n",
    "\n",
    "`-- CHANGING COLUMN HEADER FROM HYPERTENSION TO HIGHBLOOK IN KAGGLE TABLE`</br>\n",
    "ALTER TABLE KAGGLE</br>\n",
    "RENAME COLUMN HYPERTENSION TO HIGHBLOOD;</br>\n",
    "\n",
    "`-- CHANGING COLUMN HEADER FROM HIGNBLOOD TO HIGHBLOOK IN PATIENT TABLE`</br>\n",
    "ALTER TABLE PATIENT</br>\n",
    "RENAME COLUMN HIGNBLOOD TO HIGHBLOOD;</br>\n",
    "\n",
    "`-- CHANGING 'Other' TO 'Nonbinary' IN BOTH KAGGLE AND PATIENT TABLES`</br>\n",
    "UPDATE KAGGLE</br>\n",
    "SET GENDER = 'Nonbinary'</br>\n",
    "WHERE GENDER = 'Other';</br>\n",
    "\n",
    "UPDATE PATIENT\n",
    "SET GENDER = 'Nonbinary'</br>\n",
    "WHERE GENDER = 'Prefer not to answer';</br>\n",
    "\n",
    "`-- ENCODING HIGHBLOOD AND DIABETES IN KAGGLE FROM 1/0 TO YES/NO`</br>\n",
    "ALTER TABLE KAGGLE</br>\n",
    "ALTER COLUMN DIABETES TYPE TEXT;</br>\n",
    "\n",
    "ALTER TABLE KAGGLE</br>\n",
    "ALTER COLUMN HIGHBLOOD TYPE TEXT;</br>\n",
    "\n",
    "UPDATE KAGGLE</br>\n",
    "SET DIABETES = 'Yes'</br>\n",
    "WHERE DIABETES = '1';</br>\n",
    "\n",
    "UPDATE KAGGLE</br>\n",
    "SET DIABETES = 'No'</br>\n",
    "WHERE DIABETES = '0';</br>\n",
    "\n",
    "UPDATE KAGGLE</br>\n",
    "SET HIGHBLOOD = 'Yes'</br>\n",
    "WHERE HIGHBLOOD = '1';</br>\n",
    "\n",
    "UPDATE KAGGLE</br>\n",
    "SET HIGHBLOOD = 'No'</br>\n",
    "WHERE HIGHBLOOD = '0';</br>\n",
    "\n",
    "`-- ADD SOURCE COLUMN TO KAGGLE AND PATIENT TABLES`</br>\n",
    "ALTER TABLE KAGGLE</br>\n",
    "ADD SOURCE TEXT;</br>\n",
    "\n",
    "ALTER TABLE PATIENT</br>\n",
    "ADD SOURCE TEXT;</br>\n",
    "\n",
    "UPDATE KAGGLE</br>\n",
    "SET SOURCE = 'Kaggle';</br>\n",
    "\n",
    "UPDATE PATIENT</br>\n",
    "SET SOURCE = 'WGU';</br>\n",
    "\n",
    "`-- DROPPING ROWS FROM PATIENT WHERE AGE > 80 SINCE KAGGLE DATA MAX AGE IS 80`</br>\n",
    "DELETE FROM PATIENT</br>\n",
    "WHERE AGE > 80;</br>\n",
    "\n",
    "`-- DROPPING ROWS FROM KAGGLE WHERE AGE < 18 SINCE WGU DATA MINIMUM AGE IS 48`</br>\n",
    "DELETE FROM KAGGLE</br>\n",
    "WHERE AGE < 18;</br>\n",
    "\n",
    "`-- CHECKING FOR NULL VALUES FOR WGU DATA`</br>\n",
    "SELECT GENDER</br>\n",
    "FROM PATIENT</br>\n",
    "WHERE GENDER IS NULL;\n",
    "\n",
    "SELECT AGE</br>\n",
    "FROM PATIENT</br>\n",
    "WHERE AGE IS NULL;\n",
    "\n",
    "SELECT HIGHBLOOD</br>\n",
    "FROM PATIENT</br>\n",
    "WHERE HIGHBLOOD IS NULL;\n",
    "\n",
    "SELECT DIABETES</br>\n",
    "FROM SERVICESADDON</br>\n",
    "WHERE DIABETES IS NULL;\n",
    "\n",
    "`-- CHECKING FOR NULL VALUES FOR KAGGLE DATA`</br>\n",
    "SELECT GENDER</br>\n",
    "FROM KAGGLE</br>\n",
    "WHERE GENDER IS NULL;\n",
    "\n",
    "SELECT AGE</br>\n",
    "FROM KAGGLE</br>\n",
    "WHERE AGE IS NULL;\n",
    "\n",
    "SELECT HIGHBLOOD</br>\n",
    "FROM KAGGLE</br>\n",
    "WHERE HIGHBLOOD IS NULL;\n",
    "\n",
    "SELECT DIABETES</br>\n",
    "FROM KAGGLE</br>\n",
    "WHERE DIABETES IS NULL;\n",
    "\n",
    "\n",
    "`-- UNION PATIENT AND KAGGLE TABLE`</br>\n",
    "SELECT P.GENDER, P.AGE, P.HIGHBLOOD, S.DIABETES, P.SOURCE</br>\n",
    "FROM PATIENT AS P JOIN SERVICESADDON AS S USING (PATIENT_ID)</br>\n",
    "UNION ALL</br>\n",
    "SELECT GENDER, AGE, HIGHBLOOD, DIABETES, SOURCE</br>\n",
    "FROM KAGGLE;"
   ]
  },
